{
  "pack": {
    "type": "tensor_from_image",
    "image": "image",
    "transforms": [
      {
        "type": "image_to_image",
        "name": "scale",
        "width": "$scaleWidth",
        "height": "$scaleHeight"
      },
      {
        "type": "image_to_tensor",
        "name": "rgb_norm",
        "mean": [0.485, 0.456, 0.406],
        "std": [0.229, 0.224, 0.225]
      }
    ]
  },
  "unpack": {
    "type": "tuple",
    "items": [
      {
        "type": "tensor",
        "key": "pred",
        "dtype": "float"
      },
      {
        "type": "list",
        "items": [
          {
            "type": "tensor",
            "key": "output2a",
            "dtype": "float"
          },
          {
            "type": "tensor",
            "key": "output2b",
            "dtype": "float"
          },
          {
            "type": "tensor",
            "key": "output2c",
            "dtype": "float"
          }
        ]
      }
    ]
  }
}
