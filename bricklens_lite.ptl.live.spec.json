{
  "pack": {
    "type": "tensor_from_image",
    "image": "image",
    "transforms": [
      {
        "type": "image_to_image",
        "name": "center_crop",
        "width": "$scaleWidth",
        "height": "$scaleHeight"
      },
      {
        "type": "image_to_image",
        "name": "scale",
        "width": "$scaleWidth",
        "height": "$scaleHeight"
      },
      {
        "type": "image_to_tensor",
        "name": "rgb_norm",
        "mean": [0.0, 0.0, 0.0],
        "std": [1.0, 1.0, 1.0]
      }
    ]
  },
  "unpack": {
    "type": "tensor",
    "key": "pred",
    "dtype": "float"
  }
}
